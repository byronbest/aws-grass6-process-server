#!.
# this is in the context of HOME=JOBHOME
# LOCATION is initialized to SOURCE_LOCATION for each job
# and becomes exclusive to one instance of GRASS
# $HOME/.grassrc

export ALARMTIME=600

if [ -z ${COLOR+x} ]; then export COLOR=FF0000 ; fi
if [ -z ${SOURCE_LOCATION+x} ]; then export SOURCE_LOCATION=ll ; fi
if [ -z ${RESOLUTION+x} ]; then export RESOLUTION=250 ; fi
if [ -z ${RASTER_SIZE+x} ]; then ((RASTER_SIZE=(4135+2)*(7554+2))) ; export RASTER_SIZE ; fi
if [ -z ${LOCATION+x} ]; then export LOCATION=$JOBHOME ; fi
if [ -z ${MAPSET+x} ]; then export MAPSET=PERMANENT ; fi
if [ -z ${REGION+x} ]; then export REGION=bbox ; fi
if [ -z ${LINE_LOCATION+x} ]; then export LINE_LOCATION=$JOBHOME ; fi
if [ -z ${SPHERE_LOCATION+x} ]; then export SPHERE_LOCATION=sphere ; fi
if [ -z ${GMAP_LOCATION+x} ]; then export GMAP_LOCATION=gmap ; fi
if [ -z ${UNIT_LOCATION+x} ]; then export UNIT_LOCATION=aea ; fi
if [ -z ${EDWG_MAPSET+x} ]; then export EDWG_MAPSET=EDWG ; fi
if [ -z ${EDWG_COST+x} ]; then export EDWG_COST=EDWG ; fi
if [ -z ${EDWG_LAYER+x} ]; then export EDWG_LAYER=1 ; fi
if [ -z ${EDWG_COLUMN+x} ]; then export EDWG_COLUMN=RISK_CLASS ; fi
if [ -z ${EDWG_COLORS+x} ]; then export EDWG_COLORS=EDWG_colors.txt ; fi
if [ -z ${EDWG_LABELS+x} ]; then export EDWG_LABELS=EDWG_labels.txt ; fi
if [ -z ${EDWG_WEIGHTS+x} ]; then export EDWG_WEIGHTS=EDWG_weights.txt ; fi
