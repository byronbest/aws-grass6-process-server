if [ -z ${BLM_LAYER+x} ]; then export BLM_LAYER=1 ; fi
if [ -z ${BLM_CAST_COLUMN+x} ]; then export BLM_CAST_COLUMN=RentSched ; fi
if [ -z ${BLM_COLUMN+x} ]; then export BLM_COLUMN=BLM_ZONE ; fi
if [ -z ${BLM_COLORS+x} ]; then export BLM_COLORS=BLM_colors.txt ; fi
if [ -z ${BLM_LABELS+x} ]; then export BLM_LABELS=BLM_labels.txt ; fi
if [ -z ${BLM_WEIGHTS+x} ]; then export BLM_WEIGHTS=BLM_market.txt ; fi
if [ -z ${LANDCOV_MAPSET+x} ]; then export LANDCOV_MAPSET=LANDCOV ; fi
if [ -z ${LANDCOV_COST+x} ]; then export LANDCOV_COST=LANDCOV ; fi
