if [ -z ${VEG_COST_NORTH_LAYER+x} ]; then export VEG_COST_NORTH_LAYER=1 ; fi
if [ -z ${VEG_COST_SOUTH+x} ]; then export VEG_COST_SOUTH=SOUTH ; fi
