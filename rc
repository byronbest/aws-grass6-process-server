if [ -z ${LANDCOV_WEIGHTS+x} ]; then export LANDCOV_WEIGHTS=LANDCOV_weights.txt ; fi
if [ -z ${LANDCOV_COLORS+x} ]; then export LANDCOV_COLORS=LANDCOV_colors.txt ; fi
if [ -z ${SLOPE_MAPSET+x} ]; then export SLOPE_MAPSET=SLOPE ; fi
if [ -z ${SLOPE_COST+x} ]; then export SLOPE_COST=SLOPE ; fi
if [ -z ${SLOPE_COUNT+x} ]; then export SLOPE_COUNT=174 ; fi
if [ -z ${SLOPE_LAYER+x} ]; then export SLOPE_LAYER=1 ; fi
if [ -z ${SLOPE_COLUMN+x} ]; then export SLOPE_COLUMN=slope_class ; fi
if [ -z ${SLOPE_COLORS+x} ]; then export SLOPE_COLORS=SLOPE_colors.txt ; fi
if [ -z ${SLOPE_LABELS+x} ]; then export SLOPE_LABELS=SLOPE_labels.txt ; fi
